{
  "name": "socialxtypeactress",
  "version": "0.0.1",
  "private": true,
  "author": "Jake",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force",
    "clean:android": "cd android/ && ./gradlew clean && cd .. && react-native run-android",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build && rm -rf node_modules/ && npm cache clean --force && npm i",
    "test:watch": "jest --watch",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:hockeyapp": "cd android && ./gradlew assembleRelease && puck -submit=auto app/build/outputs/apk/app-release.apk",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "android:shake": "$ANDROID_HOME/platform-tools/adb devices | grep '\\t' | awk '{print $1}' | sed 's/\\s//g' | xargs -I {} $ANDROID_HOME/platform-tools/adb -s {} shell input keyevent 82",
    "lint": "tslint --project . -e '**/*.js' -t verbose | snazzy",
    "lintdiff": "git diff --name-only --cached --relative | grep '\\.tsx?$' | xargs tslint | snazzy",
    "fixcode": "tslint --project . -e '**/*.js' --fix",
    "pretty": "find ./App -name '*.ts' -o -name '*.tsx' | xargs node_modules/.bin/prettier --write --config .prettierrc",
    "prettyWin": "where /r .\\storybook\\ *.ts *.tsx | xargswin -I{} \"npm run prettier {}\"",
    "prettier": "node_modules/.bin/prettier --write --config .prettierrc",
    "git-hook": "npm run lint -s && npm run test -s",
    "precommit": "npm run pretty && npm run fixcode"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "lodash": "^4.17.5",
    "moment": "^2.20.1",
    "native-base": "^2.3.9",
    "react": "^16.2.0",
    "react-native": "^0.53.3",
    "react-native-config": "^0.11.5",
    "react-native-drawer": "^2.5.0",
    "react-native-keyboard-aware-scroll-view": "^0.4.4",
    "react-native-linear-gradient": "^2.4.0",
    "react-native-modal": "^5.1.1",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.1.1",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.7",
    "react-redux-typescript": "^3.0.0-rc.3",
    "recyclerlistview": "^1.2.6",
    "redux": "^3.7.2",
    "redux-axios-middleware": "^4.0.0",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "typesafe-actions": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^22.1.3",
    "@types/react": "^16.0.38",
    "@types/react-dom": "^16.0.4",
    "@types/react-native": "^0.52.12",
    "@types/react-native-vector-icons": "^4.4.3",
    "@types/react-navigation": "^1.0.32",
    "@types/react-redux": "^5.0.15",
    "@types/react-test-renderer": "^16.0.1",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.13.5",
    "babel-jest": "22.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^22.4.0",
    "jest-preset-typescript-react-native": "^1.2.0",
    "prettier": "^1.10.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-native-typescript-transformer": "^1.2.3",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.4",
    "tslint": "^5.9.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "jest-preset-typescript-react-native",
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/App/**/*Test.ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/__tests__/index.d.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "cacheDirectory": ".jest/cache"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
